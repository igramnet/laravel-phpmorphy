{
    "name": "igramnet/laravel-phpmorphy",
    "description": "laravel-phpmorphy - Laravel wrapper for phpMorphy.",
    "keywords": [
        "laravel",
        "phpMorphy",
        "nlp"
    ],
    "homepage": "https://github.com/igramnet/laravel-phpmorphy",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pavel Zhukovskiy",
            "email": "igramnet@gmail.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.2.0||~5.3.0||~5.4.0||~5.5.0||~5.6.0||~5.7.0||~5.8.0||^6.0||^7.0||^8.0||^9.0|^10.0||^11.0",
        "igramnet/phpmorphy": "^1.0.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "igramnet\\Morphy\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "igramnet\\Morphy\\MorphyServiceProvider"
            ],
            "aliases": {
                "Morphy": "igramnet\\Morphy\\Facade\\Morphy"
            }
        }
    }
}
